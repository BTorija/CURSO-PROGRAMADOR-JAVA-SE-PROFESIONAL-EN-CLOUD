package principal;

public class ComprobarPrimo {

	public static void main(String[] args) {
		int num = 4;
		// Indicará si el número es o no primo
	
			if(num%num==0||num%1==0)   {
				System.out.println("Es un número primo");
				
			}else {
				System.out.println("No es un número primo");
		}
		

	}

}
