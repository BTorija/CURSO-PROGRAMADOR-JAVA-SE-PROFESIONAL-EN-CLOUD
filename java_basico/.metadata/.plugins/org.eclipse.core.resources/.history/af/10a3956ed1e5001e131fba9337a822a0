package principal;

public class Division {

	public static void main(String[] args) {
		int x =8;
		int y =15;
		double result;
		//mostrar el resultado de dividir mayor entre menor respetando decimales
		if(x>y) {
			result=x/y;
		
		}else {
			result=y/x;
		}
		System.out.println(result);

	}

}
