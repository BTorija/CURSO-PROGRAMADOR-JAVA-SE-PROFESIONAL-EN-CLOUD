package principal;

public class Sumador {

	public static void main(String[] args) {
		int n1=20;
		int n2=11;
		int suma=0;
		
		
		//Calcular la suma entre todos los n√∫meros comprendidos entre el mayor 
		//y el menor, no incluidos los extremos
		
		for(int i=11;i<20;i++) {
				
				 suma+=i;	
			}
		System.out.println(suma);

	}

}
