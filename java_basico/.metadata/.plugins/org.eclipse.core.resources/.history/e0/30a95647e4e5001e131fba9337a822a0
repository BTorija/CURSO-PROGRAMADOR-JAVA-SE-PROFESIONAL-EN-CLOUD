package principal;

public class ComprobarPrimo {

	public static void main(String[] args) {
		int num = 16;
		// Indicará si el número es o no primo
		for(int i=2; i<=num-1;i++) {
			if(i%i==0 && i%1==0)   {
				System.out.println(i +"Es un número primo");
				
			}else {
				System.out.println(i +"No es un número primo");
		}
		
	
		
		

	}
	}
}


